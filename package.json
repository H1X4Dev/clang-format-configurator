{
  "name": "clang-format-configurator",
  "version": "0.0.3",
  "description": "Interactively view changes in clang-format configuration",
  "main": "server.js",
  "dependencies": {
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "marked": "^0.3.5",
    "userid": "^0.2.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "http-server": "^0.11.1"
  },
  "scripts": {
    "install": "cd ./client/; npm i",
    "start": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "server": "node server/js/server.js",
    "client": "cd ./client/; node ../node_modules/.bin/http-server -a localhost -o",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "clang"
  ],
  "author": "Ben Falconer <ben@falconers.me.uk>",
  "license": "MIT"
}
